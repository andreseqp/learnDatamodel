	pages = {3599--3639},
	file = {Full Text PDF:M\:\\Zotero\\storage\\8DI2SZHV\\O'Connell and Hofmann - 2011 - The Vertebrate mesolimbic reward system and social.pdf:application/pdf;Snapshot:M\:\\Zotero\\storage\\PQZNPXTB\\abstract.html:text/html},
